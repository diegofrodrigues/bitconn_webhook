{
  "model": "sale.order",
  "method": "search",
  "domain": [["state", "=", "sale"]],
  "limit": 3,
  "fields": [
    "id",
    "name",
    {"partner_id": ["id", "name", "email", {"country_id": ["id", "code", "name"]}]},
    {"order_line": [
      "id",
      "name",
      "price_unit",
      {"product_id": ["id", "default_code", "name", "list_price"]}
    ]}
  ]
}
